package nombres;

/*Crear un arreglo de 5 elementos de tipo String y que almacene el nombre de 5 de tus compa√±eros que si asistieron hoy*/
public class Nombres {

	public static void main(String[] args) {
		String[] nombres = new String[5];
		nombres[0] = "Fernando Zamudio";
		nombres[1] = "Hector Castro";
		//nombres[2] = "";
		nombres[3] = "Daniel Chaidez";
		nombres[4] = "Ximena Reyes";
		System.out.println(nombres);
	}

}

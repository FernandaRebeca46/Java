package tablas;
//Tablas de multiplicas desde la 11 hasta la 20//
import java.util.Scanner;

public class Tablas {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int numero;
		do {
			System.out.println("Ingrese el número: ");
			numero = scanner.nextInt();
			if (numero > 20) {
				System.out.println("Error: Ingrese un número menor o igual que 20: ");
			}
		}
			while (numero > 20);
				for (int a=1; a<=10; a++) {
					System.out.println(numero + " * " + a + " = " + numero*a);
				}
    }
}

package objecto;
//Imprimir los elementos de un arreglo sin necesidad de un Ã­ndice//

public class Objecto {

	public static void main(String[] args) throws Exception {
		Object[] arreglo = {
		10.55, true, "Lunes", 5.5f, 45, 's', //personaje1
		};
		//Por cada elemento contenido en arreglo
		for(int x = 0 ; x < arreglo.length; x++)
		{
			System.out.println(arreglo[x]);
		}
		for (Object o : arreglo) {
		/* if(o instance String)
		{
		System.out.println("Es un String");
		double numero = Double.parseDouble(o.toString());
		}
		if(o instanceof Double){
		System.out.println("Es un Double");
		double numero = (Double) o; // Casting
		System.out.println (10 + numero);
		}
		*/
			System.out.println(o);
		}
	}
}

package mayor2;
//Determinar el número mayor de 3 números ingresados//

import java.util.Scanner;

public class Mayor2 {
	
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int a, b, c;
		System.out.println("Ingresa 3 números: ");
		a = scanner.nextInt();
		b = scanner.nextInt();
		c = scanner.nextInt();
		if (a != b && a != c && b != c) {
			if (a > b) {
				if (a > c) {
					System.out.println("El número mayor es " + a);
				} 
				else {
					System.out.println("El número mayor es " + c);
					}
				}
			else {
				if (b > c){
					System.out.println("El número mayor es " + b);
				} 
				else {
					System.out.println("El número mayor es " + c);
					}
				}
			}
		else {
			System.out.println("Los números deben se diferentes entre sí");
			}
		}
	}

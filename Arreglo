package arreglo;
//Arreglo de 5 números y ordenados//

import java.util.Arrays;
public class Arreglo {

	public static void main(String[] args) {
		//Arreglo de 5 números//
		double[] numeros = new double[5];
		numeros[0] = 75.0;
		numeros[1] = 10.0;
		numeros[2] = 55.0;
		numeros[3] = 20.0;
		numeros[4] = 35.0;
		Arrays.sort(numeros);
		System.out.println(Arrays.toString(numeros));
	}
}

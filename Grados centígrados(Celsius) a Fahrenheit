package grados;
//Grados cent√≠grados(Celsius) a Fahrenheit//
import java.util.Scanner;

public class Grados {
	static Scanner leer = new Scanner(System.in);
	public static void main(String[] args) {
	double F, C;
	System.out.println("Ingrese la cantidad del grados Celsius que desee convertir: ");
	C = leer.nextDouble();
	F = (9/5.0)*C + 32;
	System.out.println("Los grados Celsius convertidos a Fahrenheit son "+F);
	}
}
